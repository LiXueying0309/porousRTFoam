FoamFile
{
    version     7;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
     
          type            fixedValue;
       value	       uniform (1e-8 0 0);
    }
    
    right
    {
         type            zeroGradient;
    }

    bottom
    {
       type            cyclic;
    }
    top
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
    
    fixedWalls
    {
        type            zeroGradient;
    }
}

