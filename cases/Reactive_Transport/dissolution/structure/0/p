FoamFile
{
    version     7;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
          type            fixedValue;
          value      uniform 1e-8;
     }

    right
    {
          type          zeroGradient;
      // value	       uniform 0;
    }

     bottom
    {
       type            cyclic;
    }
    
    top
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}

